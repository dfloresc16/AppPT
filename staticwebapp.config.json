{
  "routes": [
    {
      "route": "/dashboard/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/auth/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css"
  }
}
